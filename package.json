{
  "name": "cameron-d28.github.io",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.13",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.4.2",
    "web-vitals": "^2.1.0"
  },
  "scripts": {
    "predeploy": "yarn build",
    "deploy": "yarn deploy:cleanup && yarn deploy:copy && yarn deploy:commit && yarn deploy:push && yarn deploy:cleanup-develop",
    "deploy:cleanup": "git checkout main && rm -rf *.html *.css *.js *.svg *.png *.ico",
    "deploy:copy": "mv build/* .",
    "deploy:commit": "git add . && git commit -m 'Deploy updates'",
    "deploy:push": "git push origin main",
    "deploy:cleanup-develop": "rm -rf build && git checkout develop",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
